{
    "name": "pycmd",
    "displayName": "PYCMD",
    "description": "evaluate python expression with a hotkey in a terminal",
    "version": "0.0.1",
    "repository": "https://github.com/insilica/vscode-pycmd.git",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.pycmd"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.pycmd",
                "title": "pycmd evaluate python expression in active terminal"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "^12.20.4",
        "@types/vscode": "^1.80.0",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "glob": "^7.1.7",
        "mocha": "^10.2.0",
        "typescript": "^4.3.5"
    }
}
